Requirements :
curl, docker and docker-compose installed.

------
docker-compose up -d --build  && docker-compose logs -f


curl -L http://localhost:7000/5200400005400
curl -L http://localhost:7010/5200400005400
curl -L http://localhost:7020/5200400005400
curl -L http://localhost:7020/change_to_database

docker stop $(docker ps -a -q)
docker ps -a
docker rm microservice-with-docker-compose-and-consul-master_consul_1
docker rm microservice-with-docker-compose-and-consul-master_service-database_1
docker rm microservice-with-docker-compose-and-consul-master_service-dadata_1
docker rm microservice-with-docker-compose-and-consul-master_service-route_1
docker rm microservice-with-docker-compose-and-consul-master_service-gateway_1

docker stop $(docker ps -a -q)     && docker rm microservice-with-docker-compose-and-consul-master_consul_1  && docker rm microservice-with-docker-compose-and-consul-master_service-database_1 && docker rm microservice-with-docker-compose-and-consul-master_service-dadata_1 && docker rm microservice-with-docker-compose-and-consul-master_service-route_1 && docker rm microservice-with-docker-compose-and-consul-master_service-gateway_1 && docker ps -a
